{
  "name": "iyoramu/banana",
  "description": "Adaptable Next-Generation Advanced Nimble Architecture PHP Framework",
  "type": "project",
  "license": "MIT",
  "authors": [
      {
          "name": "IRUTABYOSE Yoramu",
          "email": "yirutabyose@gmail.com"
      }
  ],
  "require": {
      "php": "^8.1",
      "ext-json": "*",
      "ext-pdo": "*",
      "vlucas/phpdotenv": "^5.4",
      "symfony/http-foundation": "^6.2",
      "symfony/routing": "^6.2",
      "symfony/console": "^6.2",
      "symfony/event-dispatcher": "^6.2",
      "firebase/php-jwt": "^6.3",
      "monolog/monolog": "^3.3",
      "nesbot/carbon": "^2.66",
      "ramsey/uuid": "^4.7",
      "guzzlehttp/guzzle": "^7.5",
      "predis/predis": "^2.1"
  },
  "require-dev": {
      "phpunit/phpunit": "^10.2",
      "mockery/mockery": "^1.5",
      "phpstan/phpstan": "^1.10",
      "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
      "psr-4": {
          "BananaPHP\\": "app/",
          "App\\": "app/"
      },
      "files": [
          "app/Helpers/StringHelper.php",
          "app/Helpers/ArrayHelper.php",
          "app/Helpers/DateHelper.php",
          "app/Helpers/SecurityHelper.php"
      ]
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "scripts": {
      "post-autoload-dump": [
          "BananaPHP\\Console\\Kernel::postAutoloadDump"
      ],
      "test": "phpunit",
      "analyse": "phpstan analyse",
      "lint": "phpcs --standard=PSR12 app/ tests/",
      "fix": "phpcbf --standard=PSR12 app/ tests/"
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}